<p id="notice"><%= notice %></p>

<h1>Ocurrences</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Location</th>
      <th>Status</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ocurrences.each do |ocurrence| %>
      <tr>
        <td><%= ocurrence.name %></td>
        <td><%= ocurrence.description %></td>
        <td><%= ocurrence.location %></td>
        <td><%= ocurrence.status %></td>
        <td><%= ocurrence.data %></td>
        <td><%= link_to 'Show', ocurrence %></td>
        <td><%= link_to 'Edit', edit_ocurrence_path(ocurrence) %></td>
        <td><%= link_to 'Destroy', ocurrence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ocurrence', new_ocurrence_path %>
